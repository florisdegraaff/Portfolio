{
  "name": "portfolio",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --max-warnings 0",
    "check-types": "npm run typegen && tsc --noEmit",
    "typegen": "sanity schema extract --path=./lib/sanity/schema.json && sanity typegen generate",
    "typegen:watch": "sanity typegen generate --watch",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@sanity/client": "^6.15.9",
    "@sanity/vision": "^4.15.0",
    "groq": "^3.0.0",
    "next": "^16.0.1",
    "next-sanity": "^7.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sanity": "^4.15.0",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@next/eslint-plugin-next": "^15.5.0",
    "@types/node": "^22.15.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.46.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.6.0"
}
